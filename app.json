{
  "expo": {
    "name": "Cinegram",
    "slug": "cinegram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cinelogo.png",
    "scheme": "cinegram",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/cinelogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sreevenkatesh152001.cinegram"
    },
    "android": {
      "package": "com.sreevenkatesh152001.cinegram",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cinelogo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "cinegram"
          },
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/cinelogo.png",
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "01907179-860c-4fc7-92d2-4f5f0ba9e98c"
      }
    }
  }
}
