{
  "expo": {
    "name": "cinegram",
    "slug": "cinegram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/cinelogo.png",
    "scheme": "cinegram",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/cinelogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/cinelogo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sreevenkatesh152001.cinegram"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/cinelogo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "01907179-860c-4fc7-92d2-4f5f0ba9e98c"
      }
    }
  }
}
